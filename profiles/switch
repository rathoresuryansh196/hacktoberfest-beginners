#include<bits/stdc++.h>
using namespace std;
int main()
{
  int a,b,choice;
  cin>>a>>b;
  cout<<"1.add\n";
  cout<<"2.sub\n";
  cout<<"1.mul\n";
  cout<<"1.div\n";
  cout<<"enter choice\n";
  cin>>choice;
  switch(choice)
  {
   case 1:
    {cout<<a+b;break;}
   case 2:
    {cout<<a-b;break;}
   case 3:
    {cout<<a*b;break;}
   case 4:
    {cout<<a/b;break;}
   }
}
